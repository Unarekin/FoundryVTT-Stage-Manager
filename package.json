{
  "name": "foundryvtt-stage-manager",
  "version": "0.0.1",
  "description": "A module for Foundry VTT meant to replace Theatre Inserts and provide accessible methods to display and manipulate visual novel-like character art.",
  "main": "dist/module.js",
  "scripts": {
    "start": "docker compose up --build",
    "stop": "docker compose down",
    "test": "npx start-server-and-test start http://localhost:30000 'npx cypress run --browser chrome' || docker compose down",
    "test:component": "npx cypress run --component --browser chrome",
    "test:e2e": "npx start-server-and-test start http://localhost:30000 run:e2e || docker compose down",
    "run:e2e": "npx cypress run --e2e --browser chrome",
    "build": "./build.mjs",
    "build:prod": "NODE_ENV=production ./build.mjs",
    "watch": "npx onchange 'src/**/*' -- yarn build",
    "docs": "npx typedoc --out docs src/module.ts"
  },
  "author": "Unarekin",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.25.7",
    "@babel/preset-env": "^7.25.7",
    "@cypress/webpack-preprocessor": "^6.0.2",
    "@eslint/js": "^9.11.0",
    "@types/Foundry-VTT": "https://github.com/League-of-Foundry-Developers/foundry-vtt-types.git",
    "@types/cypress": "^1.1.3",
    "@types/eslint__js": "^8.42.3",
    "@types/handlebars": "^4.1.0",
    "@types/node": "^22.7.5",
    "@types/unzipper": "^0.10.10",
    "babel-loader": "^9.2.1",
    "cypress-ct-html": "^1.1.0",
    "cypress-mochawesome-reporter": "^3.8.2",
    "deepmerge-ts": "^7.1.0",
    "docker-compose": "^1.1.0",
    "esbuild": "^0.23.1",
    "esbuild-copy-static-files": "^0.1.0",
    "esbuild-node-externals": "^1.14.0",
    "esbuild-plugin-clean": "^1.0.1",
    "esbuild-plugin-json-merge": "^0.1.0",
    "esbuild-sass-plugin": "^3.3.1",
    "eslint": "^9.11.0",
    "glob": "^11.0.0",
    "handlebars": "^4.7.8",
    "path-browserify": "^1.0.1",
    "ts-loader": "^9.5.1",
    "typedoc": "^0.26.8",
    "typedoc-github-wiki-theme": "^2.0.0",
    "typedoc-plugin-markdown": "^4.2.9",
    "typedoc-umlclass": "^0.10.0",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.6.0",
    "unzipper": "^0.12.3",
    "vue": "^3.5.11",
    "webpack": "^5.95.0",
    "yocto-spinner": "^0.1.0"
  }
}
